rsync -av -t -h -P --exclude '*.*~' --exclude '*.root' -e "ssh -p 2727" wuchenguang@162.105.151.29:/data/d1/RIBLL2017NaMgAlSi/yfs/corr /Volumes/AppleData/CodeProject/Work/ribll/exp2/
